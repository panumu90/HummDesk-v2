    "@supabase/realtime-js": "^2.76.1",
    "@supabase/supabase-js": "^2.76.1",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.34.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.36.4",
    "fastify": "^4.28.1",
    "googleapis": "^164.1.0",
    "html-to-text": "^9.0.5",
    "imap": "^0.8.19",
    "ioredis": "^5.4.1",
    "mailparser": "^3.7.5",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-microsoft": "^2.1.0",
    "pg": "^8.12.0",
    "postgres": "^3.4.5",
