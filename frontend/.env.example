# ========================================
# HummDesk v2 - Frontend Environment Variables
# ========================================

# API Configuration
# Development: Point to local backend
# Production: Point to Railway backend URL
VITE_API_URL=http://localhost:5000/api/v1
VITE_WS_URL=ws://localhost:5000

# Application Metadata
VITE_APP_NAME=HummDesk
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=AI-Native Customer Service Platform

# Feature Flags
# Enable/disable features in development
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_KNOWLEDGE_BASE=true
VITE_ENABLE_TEAMS=true
VITE_ENABLE_ANALYTICS=true

# AI Configuration
# These are frontend display settings, not API keys
VITE_AI_CONFIDENCE_THRESHOLD=0.85
VITE_AI_AUTO_ASSIGN=true

# Monitoring (Optional - for error tracking)
# Get DSN from: https://sentry.io
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Analytics (Optional)
# For product analytics
VITE_ANALYTICS_ID=
